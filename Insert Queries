INSERT INTO employee
(emp_country, emp_email, emp_level, emp_hire_date, emp_name, emp_region, emp_supervisor, emp_team, emp_title)
VALUES
($country, $email, $level, $hire_date, $name, $region, $supervisor, $team, $title);


INSERT INTO company_work_history
(cwh_id, cwh_country, cwh_description, cwh_level, cwh_region, cwh_skill_team, cwh_skills_used, cwh_start_date, cwh_end_date, cwh_title)
VALUES
($id, $country, $description, $level, $region, $skill_team, $skills_used, $start_date, $end_date, $title);


INSERT INTO non_company_work_history
(ncwh_id, ncwh_company, ncwh_description, ncwh_skill_team, ncwh_skills_used, ncwh_start_date, ncwh_end_date, ncwh_title)
VALUES
($id, $company, $description, $skill_team, $skills_used, $start_date, $end_date, $title);


INSERT INTO education
(ed_id, ed_degree, ed_major, ed_minor, ed_school, ed_start_date, ed_end_date)
VALUES
($id, $degree, $major, $minor, $school, $start_date, $end_date);


INSERT INTO performance
(per_id, per_rating, per_review_author_Name, per_start_date, per_end_date) 
VALUES
($id, $rating, $review_author_Name, $start_date, $end_date);


INSERT INTO skills
(s_id, sk_skill, sk_skill_level)
VALUES
($id, $skill, $skill_level);

